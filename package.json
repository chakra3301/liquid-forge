{
  "name": "liquid-forge",
  "version": "1.0.0",
  "description": "A futuristic cyberpunk-themed NFT generator with layer-based asset management",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev:server": "cd server && npm run dev",
    "dev:client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install:all": "npm install && cd client && npm install && cd ../server && npm install",
    "vercel-build": "cd client && npm run build"
  },
  "keywords": [
    "nft",
    "generator",
    "cyberpunk",
    "react",
    "nodejs",
    "express"
  ],
  "author": "Liquid Forge Team",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "archiver": "^6.0.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "fs-extra": "^11.1.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "path": "^0.12.7",
    "sharp": "^0.32.6",
    "sqlite3": "^5.1.6",
    "unzipper": "^0.10.14",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "electron-rebuild": "^3.2.9",
    "nodemon": "^3.0.1"
  }
}
